{
  "expo": {
    "name": "movelaria-mobile",
    "slug": "movelaria-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/icoMovelaria.png",
    "splash": {
      "image": "./assets/img/splashmovelaria.png",
      "resizeMode": "contain",
      "backgroundColor": "#1d1d1d"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/icoMovelaria.png",
        "backgroundColor": "#1d1d1d"
      }
    },
    "web": {
      "favicon": "./assets/img/favicon.png"
    }
  }
}
